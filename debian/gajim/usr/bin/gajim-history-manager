#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'gajim==1.0.99','gui_scripts','gajim-history-manager'
__requires__ = 'gajim==1.0.99'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('gajim==1.0.99', 'gui_scripts', 'gajim-history-manager')()
    )
